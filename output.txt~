<script type="text/javascript">
$(function() {	
	
	$("#form-newsletter").submit(function () {
		name = $('#NAME').val();
		email = $('#EMAIL').val();
		
		if (name.length <= 0) {
			alert('O nome é obrigatório.');
			return false;
		} 

		if (email.length <= 0) {
			alert('O e-mail é obrigatório.');
			return false;
		}

		$.ajax({
	           	type: "POST",
	           	url: $("#form-newsletter").attr('action'),
	           	data: $("#form-newsletter").serialize()
	         }).done(function( msg ) {
	          	$("#form-newsletter")[0].reset();
	           	alert( msg );
	    	});
		
		
     });
});
</script>
